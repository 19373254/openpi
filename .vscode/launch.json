{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Train Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/train.py",
            "args": [
                "pi0_libero_low_mem_finetune-select_fruit100_onlycameraBCDE_yjj",
                "--exp-name=vlabench_pi0_libero_low_mem_finetune-select_fruit100_yjj_experiment_only_cameraBCDE_add_condiment_1_200_debug",
                // "--resume",
                // "--resume"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
